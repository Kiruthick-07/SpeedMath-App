{
  "expo": {
    "name": "Speed Math Trainer",
    "slug": "speed-math-trainer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F7FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.speedmath.trainer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F0F7FF"
      },
      "package": "com.speedmath.trainer"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
